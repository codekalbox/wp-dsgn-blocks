{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "version": "0.1.0",
    "name": "premium/testimonial",
    "title": "Testimonial",
    "description": "Proof your success by displaying customer reviews using Premium Testimonials Block.",
    "keywords": ["testimonial", "Quote", "review"],
    "textdomain": "premium-blocks-for-gutenberg",
    "category": "premium-blocks",
    "attributes": {
        "blockId": {
            "type": "string"
        },
        "classMigrate": {
            "type": "boolean",
            "default": false
        },
        "padding": {
            "type": "object",
            "default": {
                "Desktop": {
                    "top": "",
                    "right": "",
                    "bottom": "",
                    "left": ""
