{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "version": "0.1.0",
    "name": "premium/video-box",
    "title": "Video Box",
    "description": "Embed YouTube, Vimeo, Dailymotion, or upload a Custom Video using Premium Video Box Block.zz",
